{
    "name": "pea/rebuild-foundation",
    "authors": [
        {
            "name": "Pea",
            "email": "pea@misfist.com"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "http://wpackagist.org"
        },
        {
            "type": "package",
            "package": {
                "name": "misfist/rebuild-foundation-sites",
                "version": "v1.0",
                "type": "wordpress-plugin",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/misfist/rebuild-foundation-sites/archive/master.zip"
                },
                "require" : {
                    "composer/installers": "v1.0.6"
                }
            }
        }
    ],
    "require": {
        "wpackagist-plugin/events-manager": "5.6.1",
        "misfist/rebuild-foundation-sites": "v1.0"
    },
    "require-dev": {
        "wpackagist-plugin/developer": "1.2.5",
        "wpackagist-plugin/which-template": "4.0"
    },
    "extra": {
        "installer-paths": {
            "wp-content/plugins/{$name}/": ["type:wordpress-plugin"],
            "wp-content/themes/{$name}/": ["type:wordpress-theme"]
        }
    }
}
